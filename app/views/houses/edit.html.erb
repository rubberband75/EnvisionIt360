
    <h2>Edit House</h2>
    
    
    <%= form_for(@house, html: { multipart: true }) do |f| %>
    
      <div class="form-group" >
          <%= label_tag(:name, "House Name") %>
          <%= f.text_field :name, :class => "form-control" %>
      </div>
      
      <div class="form-group" >
          <%= label_tag(:address, "Adress") %>
          <%= f.text_field :address, :class => "form-control" %>
      </div>
      
      <% if current_user.admin? %>
      <div class="form-group" >
          <%= label_tag(:status, "Status") %>
          <%= f.text_field :status,  :class => "form-control" %>
      </div>
      <% end %>
      
      <br>
      Image
      <%= f.file_field :picture %>
      <br>
      3D File
      <%= f.file_field :file %>
      <br>
      Meta File
      <%= f.file_field :meta %>
      <br>
      
      <hr>
      <h4>Community</h4>
      <%= f.select :community, @communitiesList, :selected => @current %>
      
      
      
      
      
      <br>
      <hr>
      <h4>Options</h4>
      
      
      <% @options.each do |o| %>
        
        <%= check_box_tag("op_hash["+o.id.to_s+"]") %>
        <%=  o.name %>
        <br>
        
      <% end %>
      
      
      <br><br>
      <%= f.submit "Update", class: "btn btn-submit" %>
    <% end %>
    <br>
    <br>
